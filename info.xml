<?xml version="1.0"?>
<extension key="com.octopus8.vpcreateaccount" type="module">
  <file>vpcreateaccount</file>
  <name>com.octopus8.vpcreateaccount</name>
  <description>Volunteer Portal Create Account. Creates account for the newly registered volunteer after being accepted</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Lee Mun Jun</author>
    <email>munjun@octopus8.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://octopus8.com</url>
    <url desc="Documentation">https://octopus8.com</url>
    <url desc="Support">https://octopus8.com</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-10-04</releaseDate>
  <version>1.1</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.75</ver>
  </compatibility>
  <comments>Configuration page for dynamic custom fields</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Vpcreateaccount</namespace>
    <format>23.02.1</format>
    <angularModule>crmVpcreateaccount</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty@1.0.0</mixin>
    <mixin>menu-xml@1.0.0</mixin>    <!-- MUST INCLUDE to redirect url declared in xml file -->
  </mixins>
</extension>
